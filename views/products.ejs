                                                            <div class="review-o u-s-m-b-15">
                                                                <div class="review-o__info u-s-m-b-8">
    
                                                                    <span class="review-o__name"><%= reviw[i].username %></span>
    
                                                                    <span class="review-o__date"><%= reviw[i].time %></span>
                                                                </div>
                                                                
                                                                <div class="review-o__rating gl-rating-style u-s-m-b-8">

                                                                    <% for(var j=0; j < reviw[i].rating ; j++) { %>

                                                                        <% if(j< parseInt(reviw[i].rating) ){ %>
                                                                            <i class="fas fa-star"></i>
                                                                        <% } %>
                                                                        
                                                                        <% if( j+0.5==reviw[i].rating ) { %>
                                                                            <i class="fas fa-star-half-alt"></i>
                                                                        <% } %>

                                                                        <!-- <i class="fas fa-star"></i><i class="fas fa-star"></i>  -->
                                                                    <% }%>
                                                                    <span><%= reviw[i].rating %></span>

                                                                </div>
                                                                <p class="review-o__text"><%= reviw[i].revvcomments%></p>
                                                            </div>
                                                        <% }%>
                                                    </div>
                                                </form>
                                            </div>
