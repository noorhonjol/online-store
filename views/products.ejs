                                                        <h2><%= reviw.length %> Reviews - <%= (sum/reviw.length).toFixed(3) %> (Overall)</h2>
                                                    </div>
                                                    <div class="gl-rating-style-2 u-s-m-b-8">
                                                        
                                                        <% for(var j=0; j < (sum/reviw.length).toFixed(3) ; j++) { %>

                                                            <% if(j< parseInt((sum/reviw.length).toFixed(3)) ){ %>
                                                                <i class="fas fa-star"></i>
                                                            <% } %>
                                                            
                                                            <% if( j+0.5==(sum/reviw.length).toFixed(3) ) { %>
                                                                <i class="fas fa-star-half-alt"></i>
                                                            <% } %>

                                                            <!-- <i class="fas fa-star"></i><i class="fas fa-star"></i>  -->
                                                        <% }%>
